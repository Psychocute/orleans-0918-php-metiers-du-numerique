<form method="post" action="{{ path('job_admin_delete', {'id': job.id}) }}" onsubmit="return confirm('Etes-vous sur de vouloir supprimer cette fiche metier?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ job.id) }}">
    <button class="btnSup"><i class="fas fa-trash-alt"></i></button>
</form>
